# API Service Configuration
API_WORKERS=4
API_TIMEOUT=60
EMBEDDINGS_HOST=embeddings
EMBEDDINGS_PORT=50051
ATTRIBUTES_HOST=attributes
ATTRIBUTES_PORT=50052

# Qdrant Configuration
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=supersecret

# Embeddings Service Configuration
EMBEDDINGS_MODEL_URL=https://huggingface.co/garavv/arcface-onnx/resolve/main/arc.onnx
EMBEDDINGS_MODEL_PATH=./models/arc.onnx
EMBEDDINGS_GRPC_PORT=50051

# Attributes Service Configuration
ATTRIBUTES_MODEL_URL=https://huggingface.co/garavv/fairface-onnx/resolve/main/fairface.onnx
ATTRIBUTES_MODEL_PATH=./models/fairface.onnx
ATTRIBUTES_GRPC_PORT=50052

# Common gRPC and ONNX Runtime Configuration
GRPC_MAX_WORKERS=10
GRPC_MAX_MESSAGE_LENGTH=52428800
ONNX_PROVIDERS=CPUExecutionProvider
ONNX_INTRA_OP_THREADS=4
ONNX_INTER_OP_THREADS=2
LOGGING_LEVEL=WARNING